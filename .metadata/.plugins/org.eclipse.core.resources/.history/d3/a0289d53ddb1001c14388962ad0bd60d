package sec1;
 
	

public class Account {
	
	
	final static int MIN_BALANCE = 0;
	final static int MAX_BALANCE = 1000000;
	private int Balance;
	private int sum;
	
	
	int Account(int Balance){
		
		if(Balance<MAX_BALANCE && Balance>MIN_BALANCE)  sum += Balance; 
		else if (Balance>MAX_BALANCE || Balance<MIN_BALANCE) sum+=0;
		
		return sum+Balance;
				
	}
	


	public int getBalance() {
		return Balance;
	}


	public void setBalance(int balance) {
		Balance = balance;
	}
	
	
}


	



