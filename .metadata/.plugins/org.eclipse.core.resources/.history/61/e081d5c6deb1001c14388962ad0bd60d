package sec1;
 
	

public class Account {
	
	
	final static int MIN_BALANCE = 0;
	final static int MAX_BALANCE = 1000000;
	private int Balance;
	private int sum=0;
	
	
	int Account(int Balance){
		if(Balance<1000000 && Balance>0)  sum=Balance; 
		else if (Balance>0 || Balance<1000000) sum+=0;
		
		return Balance;
				
	}
	


	public int getBalance() {
		return Balance;
	}


	public void setBalance(int balance) {
		Balance = balance;
	}
	
	
}


	



