package sec2;

public class dustmq {

	public static void main(String[] args) {
		int[][] array = {
				{95, 86},
				{83, 92, 96},
				{78, 83, 93, 87, 88,}
		};
		
		int sum = 0;
		double avg = 0.0;
		
		for(int i=0;i<array[0].length;i++) {
			for(int j=0;j<array[1].length;j++);
		}
		
		
		System.out.println("sum: "+ sum);
		System.out.println("avg: "+ avg);
		}
}