package sec1;

public class Car1 {

	public static void main(String[] args) {
		Car sonata = new Car("소나타");
		sonata.setName("소나타9");
		sonata.setStyle("승용차");
		sonata.setUsetime(3);
		Car bmw = new Car("BMW3");
		bmw.setStyle("승합차");
		bmw.setUsetime(2);
		bmw.setOil(12);
		bmw.setFuel("경유");
		
		
		sonata.computeMoney(sonata.getStyle(),sonata.getUsetime());
	 
		bmw.computeFuel(bmw.getOil(),bmw.getFuel());
	 
	 
		
	}

}
