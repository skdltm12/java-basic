package sec1;

public class Car1 {

	public static void main(String[] args) {
		Car sonata = new Car("소나타");
		sonata.setName("소나타");
		sonata.setStyle("승용차");
		sonata.setUsetime(3);
		Car bmw = new Car("중형차");
		bmw.setStyle("승합차");
		bmw.setUsetime(5);
		
		sonata.computeMoney(sonata.getName(),sonata.getUsetime(),sonata.getStyle());
		bmw.computeMoney();
		
	}

}
