package sec4;

public class Member {
	String name, id, password;
	int age;
	
	Member (){
		this("이름","id","0000",0);
	}
	Member (String name){
		this(name,"id","0000",0);
	}
	Member (String name,String id){
		this(name,id,"0000",0);
	}
	Member (String name,String id,String password){
		this(name,id,password,0);
	}
	Member (String name,String id,String password,int age){
		this.name=name;
		this.id=id;
		this.password=password;
		this.age=age;
	}
}
