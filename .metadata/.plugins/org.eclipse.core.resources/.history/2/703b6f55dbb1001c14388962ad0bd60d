package sec1;
 
	

public class Account {
	
	
	final static int MIN_BALANCE = 0;
	final static int MAX_BALANCE = 1000000;
	private int Balance;
	
	
	Account(int Balance){
		
		if(Balance<MAX_BALANCE && Balance>MIN_BALANCE) this.Balance += Balance; 
		else {this.Balance+0}
				
	}
	


	public int getBalance() {
		return Balance;
	}


	public void setBalance(int balance) {
		Balance = balance;
	}
	
	
}


	



