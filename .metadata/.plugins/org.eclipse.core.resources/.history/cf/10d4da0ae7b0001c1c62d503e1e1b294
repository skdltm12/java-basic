package sec1;

public class Car {
	String name,company,fuel;
	int tire, speed, num, usetime;

	Car(){
		this("자동차", "null","경유", 4, 0, 0000, 0);
	}
	Car(String name){
		this(name, "null", "경유", 4, 0, 0000, 0);
	}
	Car(String name, String company){
		this(name, company, "경유", 4, 0, 0000, 0);
	}
	Car(String name, String company, String fuel){
		this(name, company, fuel, 4, 0, 0000, 0);
	}
	Car(String name, String company, String fuel, int tire){
		this(name, company, fuel, tire, 0, 0000, 0);
	}
	Car(String name, String company, String fuel, int tire, int speed){
		this(name, company, fuel, tire, speed, 0000, 0);
	}
	Car(String name, String company, String fuel, int tire, int speed, int num){
		this(name, company, fuel, tire, speed, num, 0);
	}
	
	Car(String name, String company, String fuel, int tire, int speed, int num, int usetime) {
		this.name = name;
		this.company = company;
		this.fuel = fuel;
		this.tire = tire;
		this.speed = speed;
		this.num = num;
		this.usetime = usetime;
		
		
	}
		computeMoney(){
			
		}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getCompany() {
		return company;
	}
	public void setCompany(String company) {
		this.company = company;
	}
	public String getFuel() {
		return fuel;
	}
	public void setFuel(String fuel) {
		this.fuel = fuel;
	}
	public int getTire() {
		return tire;
	}
	public void setTire(int tire) {
		this.tire = tire;
	}
	public int getSpeed() {
		return speed;
	}
	public void setSpeed(int speed) {
		this.speed = speed;
	}
	public int getNum() {
		return num;
	}
	public void setNum(int num) {
		this.num = num;
	}
	public int getUsetime() {
		return usetime;
	}
	public void setUsetime(int usetime) {
		this.usetime = usetime;
	}
	
	
}
